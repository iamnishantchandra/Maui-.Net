<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MauiTutorial.MVVM.Views.FirstPage"
             Title="FirstPage">
    <NavigationPage.TitleView>
        <Frame Margin="10"
               Padding="10">
            <SearchBar Placeholder="Search Text Here.."/>
        </Frame>
    </NavigationPage.TitleView>
    <StackLayout>
        <Entry Placeholder="Name" x:Name="nametext"/>
        <Button Text="Go to Second Page" VerticalOptions="Center" HorizontalOptions="Center" Clicked="Button_Clicked"/>
        <Button Text="Go to Third Page" VerticalOptions="Center" HorizontalOptions="Center" Clicked="Button_Clicked_1"/>
    </StackLayout>
</ContentPage>